<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:AbsolutePanel width="450px" height="300px">
			<g:at left="36" top="44">
				<g:Label text="用户名：" width="89px" height="28px" horizontalAlignment="ALIGN_LEFT" styleName="labelfont"/>
			</g:at>
			<g:at left="163" top="40">
				<g:TextBox width="180px" height="20px" ui:field="userName"/>
			</g:at>
			<g:at left="36" top="95">
				<g:Label width="89px" height="28px" horizontalAlignment="ALIGN_LEFT" styleName="labelfont" text="新密码："/>
			</g:at>
			<g:at left="163" top="91">
				<g:PasswordTextBox width="180px" height="20px" ui:field="password"/>
			</g:at>
			<g:at left="36" top="146">
				<g:Label width="110px" height="28px" horizontalAlignment="ALIGN_LEFT" styleName="labelfont" text="确认密码："/>
			</g:at>
			<g:at left="163" top="142">
				<g:PasswordTextBox width="180px" height="20px" ui:field="confirmPassword"/>
			</g:at>
			<g:at left="102" top="245">
				<g:Button text="确认" width="101px" height="30px" ui:field="submit">New Button</g:Button>
			</g:at>
			<g:at left="252" top="245">
				<g:Button width="101px" height="30px" text="取消" ui:field="cancel">New Button</g:Button>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 